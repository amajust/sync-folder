{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "android-install-tools",
  "version": "0.1.0",
  "identifier": "com.android-install-tools.app",
    "build": {
    "beforeBuildCommand": "flutter build web",
    "beforeDevCommand": "cd flutter_ui/ && flutter build web",
     "frontendDist": "../flutter_ui/build/web"

    }
  ,
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "android-install-tools",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
